{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "prettier/prettier": "error",
        "no-process-exit":"off",
        "object-shorthand":"off",
        "class-methods-use-this": "off",
        "spaced-comment": "off"
    }
}
